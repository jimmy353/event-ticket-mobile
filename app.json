{
  "expo": {
    "name": "Sirheart Events",
    "slug": "event-ticket-mobile",
    "scheme": "sirheart_events",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",

    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.sirheartevents.app",
      "permissions": ["INTERNET", "CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "extra": {
      "eas": {
        "projectId": "58dcd0dd-32d4-47f1-b87f-33c755344f33"
      }
    }
  }
}
